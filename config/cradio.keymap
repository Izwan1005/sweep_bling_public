        define_method: define_method {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp D &kp E &kp F &kp SPACE &kp UNDER &kp M &kp E &kp T &kp H &kp O &kp D &kp SPACE &kp LPAR &kp S &kp E &kp L &kp F &kp RPAR &kp COLON &kp RET>,
                <&macro_tap>,
                <&kp DQT &kp DQT &kp DQT &kp RET>,
                <&macro_tap>,
                <&kp D &kp O &kp C &kp S &kp T &kp R &kp I &kp N &kp G &kp RET>,
                <&macro_tap>,
                <&kp DQT &kp DQT &kp DQT &kp RET>,
                <&macro_tap>,
                <&kp R &kp E &kp T &kp U &kp R &kp N &kp SPACE &kp LPAR &kp S &kp E &kp L &kp F &kp RPAR>,
                <&macro_press>,
                <&macro_press>;

            label = "DEFINE_METHOD";
